<h2>New Post</h2>
<%= form_with scope: :picture, url: pictures_path, local: true do |f| %>
  <p>
    <%= f.label :caption %><br>
    <%= f.text_area :caption %>
  </p>
 
  <p>
    <%= f.label :picture %><br>
    <%= f.file_field :picture %>
  </p>
 
  <p>
    <%= f.submit("Share New Post") %>
  </p>
<% end %>


